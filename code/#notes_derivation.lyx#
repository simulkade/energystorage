\begin_body

\begin_layout Standard
Making the two-phase flow equation dimensionless
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial S_{w}}{\partial t}+\frac{u}{\varphi}\frac{df_{w}}{dS_{w}}\frac{\partial S_{w}}{\partial x}=0,
\]

\end_inset


\begin_inset Formula 
\[
X=\frac{x}{L},
\]

\end_inset


\begin_inset Formula 
\[
\tau=\frac{tu}{L\varphi},
\]

\end_inset


\begin_inset Formula 
\[
x=XL,\,\,t=\frac{\tau L\varphi}{u},
\]

\end_inset


\begin_inset Formula 
\[
\frac{u}{\varphi L}\frac{\partial S_{w}}{\partial\tau}+\frac{u}{\varphi L}\frac{df_{w}}{dS_{w}}\frac{\partial S_{w}}{\partial X}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial S_{w}}{\partial\tau}+\frac{df_{w}}{dS_{w}}\frac{\partial S_{w}}{\partial X}=0
\]

\end_inset


\begin_inset Formula 
\[
\left(\frac{\textrm{d}X}{\textrm{d}\tau}\right)_{S_{w}}=\frac{\textrm{d}f_{w}}{\textrm{d}S_{w}},
\]

\end_inset


\begin_inset Formula 
\[
X=\frac{\textrm{d}f_{w}}{\textrm{d}S_{w}}\tau
\]

\end_inset


\end_layout

\begin_layout Standard
Radial:
\begin_inset Formula 
\[
\frac{\partial S_{w}}{\partial t}+\frac{u}{r\varphi}\frac{\partial\left(rf_{w}\right)}{\partial r}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial S_{w}}{\partial t}+\frac{u}{r\varphi}\frac{\partial\left(rf_{w}\right)}{\partial r}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial S_{w}}{\partial t}+\frac{u}{r\varphi}f_{w}+\frac{u}{\varphi}\frac{\partial f_{w}}{\partial r}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial S_{w}}{\partial t}+\frac{u}{r\varphi}f_{w}+\frac{u}{\varphi}\frac{df_{w}}{dr}\frac{\partial S_{w}}{\partial r}=0
\]

\end_inset


\end_layout

\begin_layout Standard
Notes on first and second law
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
dU=\delta Q+\delta W
\]

\end_inset


\begin_inset Formula 
\[
dS=\frac{\delta Q}{T}
\]

\end_inset


\begin_inset Formula 
\[
\delta W=-pdV
\]

\end_inset


\begin_inset Formula 
\[
dU=TdS-pdV
\]

\end_inset


\begin_inset Formula 
\[
H=U+PV
\]

\end_inset


\begin_inset Formula 
\[
dH
\]

\end_inset


\end_layout

